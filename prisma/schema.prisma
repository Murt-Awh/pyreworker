generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("mongoUri")
}

model Bio {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  discordId   String @unique
  description String
}

model Referral {
  id               String @id @default(auto()) @map("_id") @db.ObjectId
  partnerDiscordId String
  code             String @unique
  discount         Int
  sales            Int
  tab              Float?
}
