generator client {
  provider = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("MONGO_URI")
}

model Bio {
  id           String  @id @default(dbgenerated()) @map("_id") @db.ObjectId
  discordId    String  @unique
  description  String
}